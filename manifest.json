{
  "manifest_version": 3,
  "name": "My Extension Again",
  "version": "1.01",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "this is a popup"
  }
}
